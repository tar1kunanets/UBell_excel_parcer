<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="642.0" prefWidth="1011.0" xmlns="http://javafx.com/javafx/8.0.999-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.michaelkunynets.excelparcer.MainController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="639.0" prefWidth="1006.0">
            <children>
                <TableView id="Work_Table" fx:id="work_table" layoutX="-4.0" layoutY="32.0" prefHeight="554.0"
                           prefWidth="1021.0" AnchorPane.bottomAnchor="53.0" AnchorPane.leftAnchor="-4.0"
                           AnchorPane.rightAnchor="-11.0" AnchorPane.topAnchor="32.0"/>
                <MenuBar layoutX="14.0" layoutY="2.0" prefHeight="25.0" prefWidth="49.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#handleOpenAction" text="Open File"/>
                                <MenuItem id="CloseBtn" mnemonicParsing="false" onAction="#handleExitAction"
                                          text="Close"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <Button id="SendButton" layoutX="902.0" layoutY="594.0" mnemonicParsing="false"
                        onMouseClicked="#SendMouseClicked" prefHeight="38.0" prefWidth="78.0" text="Send"/>
            </children>
        </AnchorPane>
    </content>
</ScrollPane>
