<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="710.0" prefWidth="1187.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.michaelkunynets.excelparcer.MainController">
   <children>
      <ScrollPane layoutY="50.0" prefHeight="660.0" prefWidth="1187.0">
         <content>
            <TableView id="work_table" fx:id="work_table" prefHeight="1000.0" prefWidth="6700.0" />
         </content>
      </ScrollPane>
      <MenuBar layoutX="14.0" layoutY="15.0" prefHeight="25.0" prefWidth="49.0">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleOpenAction" text="Open File" />
                  <MenuItem id="CloseBtn" mnemonicParsing="false" onAction="#handleExitAction" text="Close" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Button id="SendButton" layoutX="87.0" layoutY="9.0" mnemonicParsing="false" onMouseClicked="#SendMouseClicked" prefHeight="38.0" prefWidth="78.0" text="Send" />
   </children>
</AnchorPane>
